{
    "name": "krecent/api",
    "description": "Krecent wrapper for using Krecent api",
    "keywords": [
        "krecent",
        "api"
    ],
    "homepage": "https://bitbucket.com/krecent/api",
    "license": "MIT",
    "authors": [
        {
            "name": ":Waheed Derby",
            "email": "derbywaheed@gmail.com",
            "role": "Senior Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "Krecent\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Krecent\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
